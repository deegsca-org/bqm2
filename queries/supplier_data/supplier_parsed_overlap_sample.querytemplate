#standardSQL

with uniqued as (
select id, array_agg(tag order by tag) tags from (
  select * from (
    SELECT ios id, 'ios' as tag  FROM {ios_table}
    union all
    SELECT android id, 'android' as tag FROM {android_table}
  ) group by 1, 2
) group by 1
having array_length(tags) = 2
)
select * from uniqued
limit 10000
